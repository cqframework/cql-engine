{
    "java.configuration.updateBuildConfiguration": "automatic",
    "java.test.config": [
        {
            "name": "Profiling",
            "workingDirectory": "${workspaceFolder}",
            "vmargs": [ "-Xmx512M",
                "-Dcom.sun.management.jmxremote.port=9999",
                "-Dcom.sun.management.jmxremote.authenticate=false",
                "-Dcom.sun.management.jmxremote.ssl=false" ]
        }
    ],
    "java.test.defaultConfig": "Profiling",
    "cSpell.words": [
        "ANCFHIR",
        "BACKBONEELEMENT",
        "Codeable",
        "Cstor",
        "DOMAINRESOURCE",
        "Datatypes",
        "Descendents",
        "Dstu",
        "Fhir",
        "Messageheader",
        "Multisource",
        "SNOMED",
        "Schuler",
        "dtpe",
        "elems",
        "fhirpath",
        "hapi",
        "hardcoding",
        "indexth",
        "inputfile",
        "sublist",
        "sublists",
        "ucum",
        "unpair",
        "Gson",
		"numer",
		"reindex"
    ],
    "cSpell.enabledLanguageIds": [
		"java",
		"json",
		"xml",
		"md"
	],
	"cSpell.ignorePaths": [
		".git/objects",
		".vscode",
		"**/pom.xml",
		"checkstyle.xml"
	],
	"editor.codeActionsOnSave": {
		"source.organizeImports": true,
		"editor.formatOnSave": true
	},
	"files.exclude": {
		"**/.classpath": true,
		"**/.project": true,
		"**/.settings": true,
		"**/.factorypath": true
	}
}